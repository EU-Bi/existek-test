{
  "name": "AutoPrediction",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts":[
    { 
      "matches":["<all_urls>"],
      "js":["background.js"],
      "css":["style.css"]
    }
  ],
  "permissions": ["storage","activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  }
}